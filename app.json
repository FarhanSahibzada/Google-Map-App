{
  "expo": {
    "name": "Google-Maps",
    "slug": "Google-Maps",
    "entryPoint": "expo-router/entry",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "Google-Maps",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Farhansahibzada.Google-Maps"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Google-Maps to use your location."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsConfigPaths": true
    },
    "extra": {
      "OPEN_ROUTES_API_KEY": "",
      "FIREBASE_API_KEY": "",
      "FIREBASE_AUTH_DOMAIN": "",
      "FIREBASE_PROJECT_ID": "",
      "FIREBASE_STORAGE_BUCKET": "",
      "FIREBASE_MESSAGING_SENDER_ID": "",
      "FIREBASE_APP_ID": ""
    }
  }
}